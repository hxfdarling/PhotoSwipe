{
	"presets": [
		["env", {
			"modules": false,
			"targets": {
				"browsers": ["ie 11", "chrome >= 22", "> 1%"]
			}
		}],
		"stage-2"
	],
	"env": {
		"test": {
			"presets": [
				["env", {
					"modules": false
				}]
			],
			"plugins": ["istanbul"]
		}
	},
	"plugins": [
    "transform-es2015-modules-commonjs",
		"syntax-dynamic-import",
		"transform-remove-strict-mode",
		"syntax-async-functions",
		"transform-regenerator"
	]
}
